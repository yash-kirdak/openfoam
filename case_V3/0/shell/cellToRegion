FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0/shell";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    lower
    {
        type            zeroGradient;
    }
    upper
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    shell_to_solid
    {
        type            zeroGradient;
    }
    solid_to_shell
    {
        type            zeroGradient;
    }
}


